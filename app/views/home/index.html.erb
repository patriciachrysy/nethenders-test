<h1>Maximum amount of nuts reaching the city</h1>

<%= form_with url: evaluate_path, local: true, method: :post do %>

    <div>
        <%= label_tag "Datas" %><br />
        <%= text_area_tag "datas", nil, size: "27x7" %>
        <p id="notice"><%= notice %></p>
    </div>

    <%= submit_tag "Evaluate" %>

<% end %>